{
  "version": 2,
  "builds": [
    {
      "src": "weather-vue/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    },
    {
      "src": "server/api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "server/api/index.js" },
    { "src": "/(.*)", "dest": "weather-vue/$1" }
  ]
}
